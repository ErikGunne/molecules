{
    "configSchema": {
        "RaftHost": {
            "type": "string",
            "default": "localhost"
        },
        "RaftPort": {
            "description": "Port",
            "type": "integer",
            "default": 1337,
            "minium": 1024,
            "maximum": 65535
        },
        "RaftID": {
            "type": "string",
            "default": "name"
        }
    },
    "name": "molecules",
    "main": "index.js",
    "version": "0.4.0",
    "repository": "https://github.com/ErikGunne/molecules",
    "description": "A real time shared document pluging for coding together",
    "license": "MIT",
    "activationCommands": {
        "atom-workspace": "molecules:activate"
    },
    "engines": {
        "atom": ">=1.0.0 <2.0.0"
    },
    "dependencies": {
        "@grpc/proto-loader": "^0.3.0",
        "grpc": "^1.15.1",
        "request": "^2.73.0"
    },
    "scripts": {
        "postinstall": "npm rebuild --target=2.0.0 --runtime=electron --dist-url=https://atom.io/download/electron"
    }
}
